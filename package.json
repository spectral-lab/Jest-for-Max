{
  "name": "jest-for-max",
  "version": "1.0.0",
  "description": "Testing Max patchers with a delightful JavaScript Testing Framework ",
  "main": "index.js",
  "repository": "https://github.com/szk2s/Jest-for-Max",
  "author": "szk2s <suzukisjss@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "yarn prettier --write src/**/*.js index.js",
    "lint": "yarn eslint .",
    "lint:fix": "yarn eslint . --fix",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch"
  },
  "dependencies": {
    "jest": "^24.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.17.1"
  }
}
